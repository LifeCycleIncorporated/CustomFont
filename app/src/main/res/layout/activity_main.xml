<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/oneId"
        android:layout_margin="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Againts"
        android:fontFamily="@font/a"
        android:textSize="20sp"/>
    <TextView
        android:id="@+id/twoId"
        android:layout_margin="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/AldotheApache"
        android:fontFamily="@font/a"
        android:textSize="20sp"/>
    <TextView
        android:id="@+id/threeId"
        android:layout_margin="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/AmazOOSTROVv.2"
        android:textSize="20sp"/>
    <TextView
        android:id="@+id/fourId"
        android:layout_margin="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/American_Captain"
        android:textSize="20sp"/>
    <TextView
        android:id="@+id/fiveId"
        android:layout_margin="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Another_Paradise"
        android:textSize="20sp"/>




</LinearLayout>